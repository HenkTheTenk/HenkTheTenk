import csv

with open(r'C:\Users\karlh\Desktop\AOC2021\1-1.csv', newline='') as csvContents:
    reader = csv.reader(csvContents)
    listOfDepths = list(reader)

deeperCount = 0

for i, j in enumerate(listOfDepths):
    if j > listOfDepths[i - 1]:
        deeperCount += 1

print('Number of times gone deeper: ' + str(deeperCount))
